<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Export</title>

    {% apply spaceless %}
        {% do pimcore_head_link().appendStylesheet('/bundles/dropboxdigitalassetsintegration/css/bootstrap.css') %}
        {% do pimcore_head_link().appendStylesheet('/bundles/dropboxdigitalassetsintegration/css/custom.css') %}
    {% endapply %}

    {% block head_stylesheets deferred %}
        {{ pimcore_head_link() }}
    {% endblock %}
</head>
<body class="sidebar-collapse">

    <div class="wrapper">
        <div class="content-wrapper">
            {{ block('content') }}
        </div>
    </div>

    {% apply spaceless %}
        {% do pimcore_head_script().prependFile('/bundles/dropboxdigitalassetsintegration/js/pimcore/settings.js') %}
        {% do pimcore_head_script().prependFile('/bundles/dropboxdigitalassetsintegration/js/pimcore/jquery.min.js') %}
    {% endapply %}

    {% block headscripts deferred %}
        {{ pimcore_head_script() }}
    {% endblock %}

</body>
</html>